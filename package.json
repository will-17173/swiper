  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/pandorajs/mvote/LICENSE-MIT"
    }
  ],
  "author": {
    "name": "pandorajs",
    "url": "https://github.com/pandorajs"
  },
  "main": "Gruntfile.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/pandorajs/mvote.git"
  },
